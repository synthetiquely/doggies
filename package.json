{
  "name": "doggies",
  "version": "1.0.0",
  "description": "Doggies React App",
  "main": "index.js",
  "repository": "https://github.com/synthetiquely/doggies",
  "author": "Anatoly Belobrovik",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start":
      "webpack-dev-server --hot --config ./webpack.config.js --mode development",
    "build": "webpack --mode production",
    "serve": "yarn build & node ./server.js",
    "test": "jest --env=jsdom"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "dom-testing-library": "^2.2.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.3.2",
    "react-testing-library": "^3.0.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-pwa-manifest": "^3.6.2"
  },
  "dependencies": {
    "debounce": "^1.1.0",
    "emotion": "^9.1.3",
    "express": "^4.16.3",
    "loadable-components": "^2.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.3",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx}"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": ["web.js", "js", "json", "web.jsx", "jsx", "node"],
    "globals": {
      "window": true,
      "document": true
    }
  }
}
